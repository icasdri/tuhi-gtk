<interface>
    <requires lib="gtk+" version="3.12"/>
    <object class="tuhi_gtk+note_row_view+NoteRow" id="note_row">
        <child>
            <object class="GtkBox" id="box">
                <child>
                    <object class="GtkLayout">
                        <property name="vscroll-policy">minimum</property>
                        <property name="height-request">26</property>
                        <child>
                            <object class="GtkLabel" id="label">
                                <property name="halign">start</property>
                                <property name="margin-start">12</property>
                                <property name="justify">left</property>
                            </object>
                            <packing>
                                <property name="y">3</property>
                            </packing>
                        </child>
                    </object>
                    <packing>
                        <property name="expand">true</property>
                        <property name="fill">true</property>
                    </packing>
                </child>
            </object>
        </child>
    </object>
    <object class="GtkSpinner" id="spinner">

    </object>
    <object class="GtkLabel" id="failure_label">
        <property name="label">Sync Failed</property>
    </object>
    <object class="GtkImage" id="synced_in_emblem">
        <property name="icon_name">radio-checked-symbolic</property>
        <property name="icon_size">0</property>
    </object>
    <object class="GtkImage" id="unsynced_emblem">
        <property name="icon_name">radio-symbolic</property>
        <property name="icon_size">0</property>
    </object>
</interface>